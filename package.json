{
  "name": "jsontreetable",
  "version": "0.0.5",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-app": "vue-cli-service build --dest dist/app",
    "build-lib": "vue-cli-service build --dest dist/lib --target lib --name lib src/lib.js",
    "build": "yarn build-app && yarn build-lib",
    "deploy": "yarn build-app && gh-pages -d dist/app",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.11",
    "jest": "^23.4.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "vue": "^2.5.17",
    "vue2-datatable-component": "github:TrueCarry/vue2-datatable"
  },
  "main": "./dist/lib/lib.common.js",
  "files": [
    "dist/lib/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-plugin-unit-jest": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.15",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "gh-pages": "^2.1.1",
    "vue-template-compiler": "^2.5.17"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
