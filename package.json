{
  "name": "jsontreetable",
  "version": "0.0.6",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-app": "vue-cli-service build --dest dist/app",
    "build-lib": "vue-cli-service build --dest dist/lib --target lib --name lib src/lib.js",
    "build": "yarn build-app && yarn build-lib",
    "deploy": "yarn build-app && gh-pages -d dist/app",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "bootstrap-vue": "^2.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "jsonex-treedoc": "../jsonextreedocjs",
    "lodash": "^4.17.15",
    "vue": "^2.6.10",
    "vue-resize": "^0.4.5",
    "vue2-datatable-component": "github:TrueCarry/vue2-datatable"
  },
  "main": "./dist/lib/lib.common.js",
  "files": [
    "dist/lib/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0-rc.3",
    "@vue/cli-plugin-eslint": "^4.0.0-rc.3",
    "@vue/cli-plugin-unit-jest": "^4.0.0-rc.3",
    "@vue/cli-service": "^4.0.0-rc.3",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "vue-template-compiler": "^2.6.10",
    "gh-pages": "^2.1.1"

  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
